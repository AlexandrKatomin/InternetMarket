<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dao.model.Computer" table="computer">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="countCPU" column="countcpu" type="int"/>
        <property name="countRAM" column="countram" type="int"/>
        <property name="countMemoryHDD" column="countmemoryhdd" type="int"/>
        <many-to-one name="goods" class="dao.model.Goods">
            <column name="goods_id" not-null="true"></column>
        </many-to-one>
    </class>


</hibernate-mapping>